{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.org/schemas/federation/federated_response.schema.json",
  "title": "FederatedOracleResponse",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "federation_id",
    "query_id",
    "vault_id",
    "mode",
    "score_bucket",
    "response_hash",
    "signature"
  ],
  "properties": {
    "federation_id": {
      "type": "string"
    },
    "query_id": {
      "type": "string"
    },
    "vault_id": {
      "type": "string"
    },
    "mode": {
      "type": "string"
    },
    "score_bucket": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "lower",
        "upper"
      ],
      "properties": {
        "lower": {
          "type": "number"
        },
        "upper": {
          "type": "number"
        }
      }
    },
    "e_value": {
      "type": [
        "number",
        "null"
      ],
      "minimum": 0
    },
    "response_hash": {
      "type": "string"
    },
    "signature": {
      "type": "string"
    }
  }
}